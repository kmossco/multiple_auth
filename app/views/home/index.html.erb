<% if current_user %>
  <%= current_user.to_json %>
<% else %>
  <%= button_to "Sign in with Azure",
                user_azure_activedirectory_v2_omniauth_authorize_path,
                method: :post %>

  <%= button_to "Sign in with Google",
                user_google_oauth2_omniauth_authorize_path,
                method: :post %>
<% end %>
